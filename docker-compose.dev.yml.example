datav:
    image: busybox
    volumes:
        - /virtuoso_db/

virtuoso:
    image: eeacms/virtuoso:dev
    ports:
        - 8890:8890
        - 1111:1111
        - 1112:1112
    volumes_from:
        - datav
    env_file:
        - virtuoso.env
    volumes:
        - /var/loval/cr3/files:/var/local/cr3/files
        - /var/backups/sql:/var/backups/sql
        - /var/tmp:/var/tmp
